{
  "name": "cronflow",
  "code": "cronflow",
  "version": "0.0.1",
  "description": "Make pipeline cron job easier and amazing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cronflow/cronflow.git"
  },
  "author": "Sirius Guo <cealer@foxmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r babel-register\"",
    "local": "docker-compose -f docker-compose.dev.yml up",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w src test",
    "y": "npm i --no-package-lock --registry=https://registry.npm.taobao.org",
    "sass": "./node_modules/.bin/sass public/assets/scss/style.scss public/assets/css/style.css --trace"
  },
  "keywords": [
    "cron",
    "cronjob",
    "pipeline",
    "flow",
    "cronflow"
  ],
  "dependencies": {
    "alphabetjs": "^0.1.4",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "colors": "^1.3.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.3",
    "pug": "^2.0.3",
    "rimraf": "^2.6.2",
    "sequelize": "^4.37.10",
    "tracer": "^0.8.15"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-watch": "^3.1.5",
    "jest": "^23.1.0",
    "nodemon": "^1.17.5",
    "sass": "^1.5.1",
    "supertest": "^3.1.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
